@model DataSet
@using System.Data
@using (Html.BeginForm())

{
<div class="outer-box">
    <div class="login-box">
        <table cellpadding="0" cellspacing="0">
            <tr>
                <th>hastaID</th>
                <th>Adı Soyadı</th>
                <th>Kimlik No</th>
                <th>Baba Adı</th>
                <th>Anne Adı</th>
                <th>Dogum Tarihi</th>
                <th>Dogum Yeri</th>
                <th>Cinsiyet</th>
                <th>Boy</th>
                <th>Kilo</th>
                <th>Meslek</th>
