                <th>Aile Hastalıkları</th>
                <th>Gecmis Ameliyatları</th>
                <th>Sakin Hakkinda</th>

            </tr>
            @foreach (DataRow row in Model.Tables[0].Rows)
            {
    <tr>
        <td>@row["hastaID"] </td>
        <td>@row["hastaAdi"] </td>
        <td>@row["kimlikNo"] </td>
        <td>@row["babaadi"] </td>
        <td>@row["anneadi"] </td>
        <td>@row["dogumtarihi"] </td>
        <td>@row["dogumyeri"] </td>
        <td>@row["cinsiyet"] </td>
        <td>@row["boy"] </td>
        <td>@row["kilo"] </td>
        <td>@row["meslek"] </td>
        <td>@row["kayittarihi"] </td>
        <td>@row["il"] </td>
        <td>@row["ilçe"] </td>
        <td>@row["adres"] </td>
        <td>@row["evTel"] </td>
        <td>@row["ceptel"] </td>
        <td>@row["email"] </td>
        <td>@row["sosguvkur"] </td>
        <td>@row["sigara"] </td>
        <td>@row["alkol"] </td>
        <td>@row["kangrubu"] </td>
        <td>@row["hepatit"] </td>
        <td>@row["hiv"] </td>
        <td>@row["diyabet"] </td>
        <td>@row["kullandigilac"] </td>
        <td>@row["gecmishastaliklar"] </td>
        <td>@row["ailehastaliklari"] </td>
        <td>@row["gecmisameliyat"] </td>
        <td>@row["sakinhakkinda"] </td>

    </tr>}
        </table>
        <input type="text" name="sakinkodu" class="form-control" placeholder="Sakin Kodu" />




        <button id="btn-login">Sil</button>

    </div>

</div>}